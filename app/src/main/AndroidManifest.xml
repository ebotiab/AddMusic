<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.addmusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WesternMusicActivity"></activity>
        <activity android:name=".ThrillerMusicActivity" />
        <activity android:name=".HappyMusicActivity" />
        <activity android:name=".RomanticMusicActivity" />
        <activity android:name=".HorrorMusicActivity" />
        <activity android:name=".DramaMusicActivity" />
        <activity android:name=".ComedyMusicActivity" />
        <activity android:name=".ActionMusicActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>